<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>LiveSearch</display-name>
    <servlet>
        <servlet-name>LiveSearch</servlet-name>
        <servlet-class>LiveSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSearchResponse</servlet-name>
        <servlet-class>GetSearchResponse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadFile</servlet-name>
        <servlet-class>DownloadFile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LiveSearch</servlet-name>
        <url-pattern>/LiveSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSearchResponse</servlet-name>
        <url-pattern>/GetSearchResponse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadFile</servlet-name>
        <url-pattern>/DownloadFile</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
